<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.oc.rss.frommindtomusic">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="FMTM"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RecordActivity" />
        <activity android:name=".ChoiceActivity" />
        <activity android:name=".ResultActivity" />
        <activity android:name=".InstrumentsChoiceActivity" />
        <activity android:name=".HarmonizeActivity" />
        <activity android:name=".OrchestrateActivity" />
        <activity android:name=".DoItYourselfActivity" />
        <activity android:name=".SingInstrumentActivity" />
        <activity android:name=".SavedActivity" />
        <activity android:name=".StyleActivity"></activity>
        <activity android:name=".FirstInstrumentActivity"></activity>
    </application>

</manifest>